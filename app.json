{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5d610aed-a159-49c9-806a-a41289e7d7be"
      }
    },
    "android": {
      "compileSdkVersion": 35,
      "targetSdkVersion": 33,
      "buildToolsVersion": "35.0.0",
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.kodyakd.boltexponativewind"
    }
  }
}
